{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
	  "no-underscore-dangle": ["error",{"allow":["_id"]}],
	  "no-unused-vars": ["error", { "args": "none" }],
	  "no-useless-escape":"off"
	}
  }